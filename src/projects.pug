extends ./layouts/default.pug

block content
  main(data-senna-surface).main.container#main
    div.projects  
      section.projects-list
        h1.heading-lv1 Projetos

        for project in projects
          article.project
            h2.project-name=project.name
            div.project-star
              span.project-star-count=project.star
            p.project-description=project.description
      
